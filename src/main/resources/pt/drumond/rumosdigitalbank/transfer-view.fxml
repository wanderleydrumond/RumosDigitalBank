<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="140.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.drumond.rumosdigitalbank.TransferController">
    <children>
        <TextField fx:id="textFieldAccount" layoutX="26.0" layoutY="25.0" onKeyTyped="#formatTextFieldAccountToNumbersOnly" promptText="Destination account" />
        <TextField fx:id="textFieldValue" layoutX="26.0" layoutY="63.0" onKeyTyped="#formatTextFieldValueToNumbersOnly" promptText="Transfer value" />
        <Button cancelButton="true" layoutX="27.0" layoutY="100.0" mnemonicParsing="false" onAction="#cancel" text="CANCEL" />
        <Button alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="142.0" layoutY="100.0" mnemonicParsing="false" onAction="#transfer" text="OK" textAlignment="RIGHT" />
    </children>
</AnchorPane>
